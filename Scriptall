local VirtualUser = game:GetService("VirtualUser")
local camera = workspace.CurrentCamera

-- ğŸ–± Click 1 láº§n giá»¯a mÃ n hÃ¬nh trÆ°á»›c
task.spawn(function()
    task.wait(0.01) -- chá» 0.5s Ä‘á»ƒ cháº¯c cháº¯n mÃ n hÃ¬nh Ä‘Ã£ sáºµn sÃ ng
    local center = camera.ViewportSize / 2
    VirtualUser:Button1Down(center)
    task.wait(0.01)
    VirtualUser:Button1Up(center)
end)

-- ğŸ“¥ Sau Ä‘Ã³ load script Shadow-Vertex
task.delay(0.5, function() -- chá» 1s sau khi click xong
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Shadow-Vertex/Script-client/refs/heads/main/scrt"))()
end)
