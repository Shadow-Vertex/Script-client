CreateButton("2Ô∏è‚É£ Fluent UI", 100, function()
    local ScreenGui2 = Instance.new("ScreenGui")
    ScreenGui2.Name = "FluentToggleIcon"
    ScreenGui2.Parent = game.CoreGui

    local ImageButton = Instance.new("ImageButton")
    ImageButton.Parent = ScreenGui2
    ImageButton.BackgroundColor3 = Color3.fromRGB(20,20,20)
    ImageButton.Position = UDim2.new(0.1,0,0.2,0)
    ImageButton.Size = UDim2.new(0,40,0,40)
    ImageButton.Image = "rbxassetid://11154200428"
    local UICorner2 = Instance.new("UICorner")
    UICorner2.CornerRadius = UDim.new(1,0)
    UICorner2.Parent = ImageButton

    ImageButton.MouseButton1Click:Connect(function()
        game:GetService("VirtualInputManager"):SendKeyEvent(true, Enum.KeyCode.End, false, game)
        task.wait()
        game:GetService("VirtualInputManager"):SendKeyEvent(false, Enum.KeyCode.End, false, game)
    end)

    local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
    local Window2 = Fluent:CreateWindow({Title = "üî• All SDVT Executor Script (Fluent UI)", SubTitle = "By SDVT Team", TabWidth = 180})

    -- ü•≠ BloxFruit
    local Blox2 = Window2:AddTab({Title="ü•≠ BloxFruit"})
    Blox2:AddButton({Title="‚ö° Speed Hub X (Auto Key)", Callback=function()
        script_key="BRiKqnqHmXcWBYjXFVtxYBMkydfKjnbK";
